    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.1.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "3.3.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "ng-packagr": "9.1.5",
    "node-jq": "1.11.1",
    "npm-run-all": "4.1.5",
    "ora": "4.0.4",
    "prettier": "2.0.5",
    "protractor": "7.0.0",
    "rimraf": "3.0.2",
    "shelljs": "0.8.4",
    "simple-git": "2.9.0",
    "ts-node": "8.10.2",
    "tslint": "6.1.2",
