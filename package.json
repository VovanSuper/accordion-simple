  "license": "GNU GPLv3",
  "description": "A very primitive accordion component to use in Angular / Ionic projects .. (WIP) and sample app (Ionic) using it",
  "keywords": [
    "accordion",
    "accordion-component",
    "accordion-simple",
    "Angular",
    "Angular9",
    "Angular10",
    "Ionic",
    "Ionic4",
    "Ionic5",
    "typescript",
    "javascript"
  ],
  "private": false,
  "author": {
    "name": "Vladimir Ovsyukov",
    "email": "ovsyukov@yandex.com"
  },
  "engines": {
    "node": ">=12.11.1",
    "npm": ">=6.12.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ngx-dummy/accordion-simple.git"
  },
  "bugs": {
    "email": "ovsyukov@yandex.com",
    "url": "https://github.com/ngx-dummy/accordion-simple/issues"
  },
  "homepage": "https://github.com/ngx-dummy/accordion-simple#README",
  "scripts": {
    "sample": "ng serve accordion-sample -o --host 0.0.0.0 --port 8881 --live-reload",
    "sample:prod": "ng serve accordion-sample -o --host 0.0.0.0 --port 8882 --live-reload --prod",
    "clean:dist": "rimraf ./dist/**",
    "clean:docs": "rimraf ./docs/*",
    "build": "ng b",
    "build:prod": "ng b --prod",
    "build:sample": "ng b accordion-sample",
    "build:sample:prod": "ng b accordion-sample --prod  -- --base-href /accordion-simple/",
    "publish:exec": "sh ./scripts/publish.sh",
    "publish:full": "run-s clean:dist build:prod publish:exec",
    "docs:gh-pages": "sh ./scripts/docs.sh",
    "docs:typedocs": "sh ./scripts/typedocs.sh",
    "docs": "run-s clean:docs docs:gh-pages docs:typedocs",
    "prepush:origin": "npm run docs",
    "push:origin": "sh ./scripts/push.sh",
    "test": "ng test",
    "test:ci": "ng test --karma-config=karma-ci.conf.js --no-watch --no-progress",
    "prerelease": "npm run docs",
    "release": "sh ./scripts/release.sh"
  },
  "dependencies": {
    "@angular/animations": "9.1.11",
    "@angular/common": "9.1.11",
    "@angular/compiler": "9.1.11",
